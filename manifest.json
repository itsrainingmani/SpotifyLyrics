{
  "manifest_version": 2,
  "name": "SpotifyLyrics",
  "version": "0.1",
  "content_scripts": [{
      "matches": [
        "https://open.spotify.com/*"
      ],
      "js": ["jquery-3.2.1.min.js", "content.js"]
    }
  ],
  "browser_action": {
    "default_icon": "icon.png"
  },
  "background": {
    "scripts": ["background.js"]
  }
}
